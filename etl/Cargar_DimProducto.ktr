    <connection>Origen_AdventureWorks</connection>
    <sql>SELECT 
    p.productid AS id_producto_original,
    p.name AS nombre_producto,
    ps.name AS subcategoria,       -- Devolverá NULL si no tiene
    pc.name AS categoria,          -- Devolverá NULL si no tiene
    p.color,                       -- Devolverá NULL si no tiene
    p.size AS tamanio,             -- Devolverá NULL si no tiene
    pm.name AS modelo,             -- Devolverá NULL si no tiene
    p.productline AS tipo_producto, -- Devolverá NULL, R, M, T, S
	p.modifieddate AS fecha_carga
FROM production.product p
LEFT JOIN production.productsubcategory ps ON p.productsubcategoryid = ps.productsubcategoryid
LEFT JOIN production.productcategory pc ON ps.productcategoryid = pc.productcategoryid
LEFT JOIN production.productmodel pm ON p.productmodelid = pm.productmodelid</sql>
